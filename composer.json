{
    "name": "behat/chained-steps-extension",
    "type": "behat-extension",
    "description": "Chained steps support for behat v3",
    "keywords": ["test", "cleanup", "human-readable"],
    "homepage": "http://behat.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Konstantin Kudryashov",
            "email": "ever.zet@gmail.com"
        }
    ],

    "require": {
        "php":          ">=5.3.2",
        "behat/behat":  "~3.0.0@dev"
    },

    "require-dev": {
        "symfony/process": "~2.1",
        "phpunit/phpunit": "~3.7.28"
    },

    "autoload": {
        "psr-0": { "Behat\\ChainedStepsExtension": "src/" }
    },

    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
